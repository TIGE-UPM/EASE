div.outcomes_header{
	margin-top:15px;
	overflow:auto;
	margin-bottom:20px;
}
table.outcomes_header{
	float:left;
}
table.outcomes_header td{
	text-align:left;
	background:#FFF;
	font-size:14px;
}
table.outcomes_table th{
	background:#FFF;
	padding:3px;
	font-size:14px;
	border-bottom:#EDC 2px solid;
}

	td.hor_group_even{
		background:#FED;
	}
	td.hor_group_odd{
		background:#FEC;
	}
table.outcomes_table{
	display:block;
}
div.outcomes_tab td{
	font-size:12px;
}
div#tabs{
	margin-bottom:15px;
}

.vertical_group_open{
	border-left:2px solid #EDC;
}
.invisible{
	border:0px;
	background:transparent;
}

.categories{
	text-align:left;
}
.outcomes_publication_menu{
	float:right;
	width:180px;
	height:60px;
	background: #FEFEFE;
}
